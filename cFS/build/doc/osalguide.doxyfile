#---------------------------------------------------------------------------
# Doxygen Configuration options to generate the "OSAL API Guide"
#---------------------------------------------------------------------------

# Start with the main page so document ordering is established
INPUT                 += /cFS_bootes/cfe/docs/src/osalmain.dox

# Common definitions, some of which are extended or overridden here.
@INCLUDE               = /cFS_bootes/build/doc/osal-common.doxyfile
PROJECT_NAME           = "OSAL User's Guide"
OUTPUT_DIRECTORY       = osalguide
GENERATE_LATEX         = YES

#PREDEFINED
PREDEFINED            += 

# Bring in the cFE header files for the documentation of the various API calls
INPUT                 += /cFS_bootes/osal/src/os/inc/common_types.h \
/cFS_bootes/osal/src/os/inc/osapi-os-core.h \
/cFS_bootes/osal/src/os/inc/osapi-os-filesys.h \
/cFS_bootes/osal/src/os/inc/osapi-os-loader.h \
/cFS_bootes/osal/src/os/inc/osapi-os-net.h \
/cFS_bootes/osal/src/os/inc/osapi-os-timer.h \
/cFS_bootes/osal/src/os/inc/osapi-version.h \
/cFS_bootes/osal/src/os/inc/osapi.h \
/cFS_bootes/build/doc/osconfig-example.h
